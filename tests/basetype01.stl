
type API =
  { truncate+ : Float -> Int
  , fromInt+  : Int -> Float
  , showInt+  : Int -> String
  }

#check forall i f s.
       { truncate+ : f -> i
       , fromInt+  : i -> f
       , showInt+  : i -> s
       }
       <: API

type Array a = exists (n : Nat).
  { size+     : Nat n
  , elements+ : a[n]
  }[n]

#eval Array
#eval Array (<Int : Int, Float : Float>)
